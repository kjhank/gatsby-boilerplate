    "polished": "^4.1.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sanitize-html": "^2.6.1",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^5.3.3"
