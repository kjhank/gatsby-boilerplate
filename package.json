    "@types/sanitize-html": "^2.9.5",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "gatsby-plugin-eslint": "^4.0.4",
    "gatsby-plugin-tsconfig-paths": "^1.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss-scss": "^4.0.9",
    "postcss-styled": "^0.34.0",
    "postcss-syntax": "^0.36.2",
    "stylelint": "^14.16.1",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^5.0.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "styled-components",
    "eslint"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "lint": "eslint . && stylelint './src/**/*.ts'",
    "lint:css": "stylelint './src/**/*.styled.ts'",
    "lint:ts": "eslint .",
    "lint:types": "tsc --noEmit",
    "nuke": "gatsby clean && rm -fr node_modules && yarn install",
    "pre-commit": "lint-staged",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "start": "yarn run develop",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kjhank/gatsby-styled-components"
  },
  "bugs": {
    "url": "https://github.com/kjhank/gatsby-styled-components/issues"
  },
  "packageManager": "pnpm@8.7.1"
}
