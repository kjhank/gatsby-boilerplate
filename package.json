{
  "name": "gatsby-ts-boilerplate",
  "private": true,
  "description": "Gatsby TS starter with Styled Components + ESLint",
  "version": "1.3.0",
  "author": "Krzysztof Hankiewicz @kjhank <kjhank@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^2.1.4",
    "gatsby": "^5.12.6",
    "gatsby-plugin-image": "^3.12.1",
    "gatsby-plugin-manifest": "^5.12.1",
    "gatsby-plugin-offline": "^6.12.1",
    "gatsby-plugin-sharp": "^5.12.1",
    "gatsby-plugin-styled-components": "^6.12.0",
    "gatsby-plugin-typescript": "^5.12.0",
    "gatsby-source-filesystem": "^5.12.0",
    "gatsby-transformer-sharp": "^5.12.1",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanitize-html": "^2.11.0",
    "styled-components": "^6.0.9"
