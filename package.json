    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-webpack-plugin": "^4.0.1",
    "gatsby-plugin-eslint": "^4.0.4",
    "gatsby-plugin-tsconfig-paths": "^1.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "parcel": "2.9.1",
    "postcss-scss": "^4.0.7",
    "postcss-styled-syntax": "^0.4.0",
    "postcss-syntax": "^0.36.2",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "styled-components",
    "eslint"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "lint": "eslint . && stylelint './src/**/*.ts'",
    "lint:css": "stylelint './src/**/*.styled.ts'",
    "lint:ts": "eslint .",
    "lint:types": "tsc --noEmit",
    "nuke": "gatsby clean && rm -fr node_modules && yarn install",
    "pre-commit": "lint-staged",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "start": "yarn run develop",
    "postinstall": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kjhank/gatsby-styled-components"
  },
  "bugs": {
    "url": "https://github.com/kjhank/gatsby-styled-components/issues"
  },
  "packageManager": "pnpm@8.7.1"
}
