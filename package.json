{
  "name": "gatsby-styled-components",
  "private": true,
  "description": "basic Gatsby starter with Styled Components & linters",
  "version": "1.0.0",
  "author": "Krzysztof Hankiewicz @kjhank <kjhank@gmail.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.13.3",
    "gatsby": "^4.5.2",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-image": "^2.0.0",
    "gatsby-plugin-manifest": "^4.0.0",
    "gatsby-plugin-offline": "^5.0.0",
    "gatsby-plugin-react-helmet": "^5.0.0",
    "gatsby-plugin-sharp": "^4.0.1",
    "gatsby-plugin-styled-components": "^5.0.0",
    "gatsby-source-filesystem": "^4.0.0",
    "gatsby-transformer-sharp": "^4.0.0",
    "polished": "^4.1.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sanitize-html": "^2.6.1",
    "smoothscroll-polyfill": "^0.4.4",
    "styled-components": "^5.3.3",
    "stylelint-config-recommended": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/eslint-plugin": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/runtime": "^7.15.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^8.0.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "stylelint": "13.8.0",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "4.1.0",
    "stylelint-processor-styled-components": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn eslint",
      "yarn stylelint \"app/**/*.js\""
    ],
    "*.jsx": [
      "yarn eslint"
    ]
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "lint": "eslint . && stylelint \".src/**/*.js\"",
    "lint:css": "stylelint \"./src/**/*.js\"",
    "nuke": "gatsby clean && rm -fr node_modules && yarn install",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "start": "yarn run develop",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kjhank/gatsby-styled-components"
  },
  "bugs": {
    "url": "https://github.com/kjhank/gatsby-styled-components/issues"
  },
  "packageManager": "yarn@3.1.0"
}
